{
  "name": "oauth2-popup-flow",
  "version": "0.1.0",
  "description": "A very simple oauth2 implicit flow library that uses window.open.",
  "main": "./dist/oauth2-popup-flow.js",
  "typings": "./src/index.ts",
  "scripts": {
    "test": "nyc jasmine-ts \"src/**/*.spec.ts\" | coveralls",
    "build": "webpack -p",
    "docs": "typedoc --out docs/ src/"
  },
  "author": "Rico Kahler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ricokahler/oauth2-popup-flow.git"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "awesome-typescript-loader": "^3.5.0",
    "coveralls": "^3.0.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "jsdom": "^11.6.2",
    "nyc": "^11.4.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "text-lcov"
    ],
    "extension": [
      ".ts"
    ]
  }
}
