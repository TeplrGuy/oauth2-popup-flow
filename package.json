  "main": "./dist/oauth2-popup-flow.js",
  "typings": "./src/index.ts",
  "scripts": {
    "test": "nyc jasmine-ts \"src/**/*.spec.ts\"",
    "build": "webpack -p"
  },
  "author": "Rico Kahler",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "awesome-typescript-loader": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^11.4.1",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
